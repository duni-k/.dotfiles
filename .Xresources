! ^ The above lines are no comments!
! Leave them as they are if a file ~/.extend.Xresources is being used on your system.
! config can be added there or also here below.

!!! Xterm config !!!
XTerm.termName: xterm-256color
XTerm*faceName: xft:Inconsolata:size=16
XTerm*reverseVideo: False
XTerm*selectToClipboard: true
XTerm*cursorBlink: True
Xcursor.theme: Maia-Cursor
XTerm*borderWidth: 0
XTerm.vt100.saveLines: 4096
Xterm*allowTitleOps: false
! dynamic font resize
Xterm*VT100.Translations: #override \
    Meta <Key> minus: smaller-vt-font() \n\
    Meta <Key> plus: larger-vt-font() \n
\
XTerm.vt100.printerCommand: xterm -e sh -c 'less <&3' 3<&0
XTerm.vt100.translations: #override Ctrl ~Meta ~Shift <Key>slash: print-everything()


Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

